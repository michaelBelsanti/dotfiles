{
   "author": "John Dunning",
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon-19.png",
         "38": "img/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "QuicKey"
   },
   "commands": {
      "1-previous-tab": {
         "description": "Switch to the previous tab",
         "suggested_key": {
            "default": "Alt+A",
            "mac": "MacCtrl+A"
         }
      },
      "2-next-tab": {
         "description": "Switch to the next tab",
         "suggested_key": {
            "default": "Alt+S",
            "mac": "MacCtrl+S"
         }
      },
      "30-toggle-recent-tabs": {
         "description": "Switch instantly between the two most recent tabs",
         "suggested_key": {
            "default": "Alt+Z",
            "mac": "MacCtrl+Z"
         }
      },
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+Q",
            "mac": "MacCtrl+Q"
         }
      }
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "Add keyboard shortcuts to switch tabs with a Quicksilver-style search or a most recently used menu",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "48": "img/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "60",
   "name": "QuicKey â€“ The quick tab switcher",
   "offline_enabled": true,
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "bookmarks", "history", "storage", "sessions", "chrome://favicon/" ],
   "short_name": "QuicKey",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6.1"
}
